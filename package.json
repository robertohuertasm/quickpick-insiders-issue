{
  "name": "quickpick-insiders-issue",
  "displayName": "Quickpick Insiders Issue",
  "description": "Extension showcasing an issue with VS Code Insiders, commands and quickpicks",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quickpick-insiders-issue.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "quickpick-insiders-issue.quickPickTest",
        "title": "QuickPick Test",
        "icon": "resources/testQuickPickIcon.png"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "tree",
          "id": "quickpick.tester.view",
          "initialSize": 1,
          "name": "QuickPick Tester",
          "visibility": "visible",
          "contextualTitle": "QuickPick Insiders Issue"
        }
      ]
    }, 
    "menus": {
      "view/title": [
        {
          "command": "quickpick-insiders-issue.quickPickTest",
          "when": "view === quickpick.tester.view",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
